<!-- -*- mode: XML -*- -->
<launch>

  <arg name="racecar_version" default="racecar-v2" />

  <arg name="config"     default="default" />
  <arg name="config_path" default="$(find mowbot_v1_controller)/config/$(arg config).yaml" />

  <node pkg="racecar_safety_controller" type="mowbot_v1_controller_node.py" name="mowbot_v1_controller">
    <remap from="navigation" to="/vesc/high_level/ackermann_cmd_mux/input/nav_0" />
  </node>

</launch>
